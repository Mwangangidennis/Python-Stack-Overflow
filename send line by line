# Message Input
message_txt = open('Wamessage.dat', 'r')
msg_full = message_txt.readlines()

# split lines
msg = []
for line in msg_full:
    msg.append(line.strip())
    # send message
message_txt.close()
# end split lines
# message end

a = 0
for a in msg:
    # msg_box = driver.find_element_by_xpath('//*[@id="main"]/footer/div[1]/div[2]/div/div[2]')
    # msg_box.send_keys(a)
    # msg_box.send_keys(Keys.SHIFT + Keys.RETURN)
    print(a)
    # looping between sending the text
